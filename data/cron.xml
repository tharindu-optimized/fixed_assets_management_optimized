<?xml version="1.0" encoding="utf-8"?>
<odoo>

    <record id="server_details_cron" model="ir.cron">
        <field name="name">Server Details Subscription and Notifications</field>
        <field name="model_id" ref="model_server_details"/>
        <field name="interval_number">1</field>
        <field name="interval_type">days</field>
        <field name="numbercall">-1</field>
        <field name="state">code</field>
        <field name="code">model.server_subscription_and_notifications()</field>
        <field name="nextcall" eval="(DateTime.now() + timedelta(days=1)).strftime('%Y-%m-%d 05:00:00')"/>
    </record>

</odoo>
